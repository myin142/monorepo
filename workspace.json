{
    "version": 1,
    "projects": {
        "admin": {
            "projectType": "application",
            "root": "apps/admin",
            "sourceRoot": "apps/admin/src",
            "architect": {
                "build": {
                    "builder": "@nx-plus/vue-plugin:browser",
                    "options": {
                        "publicPath": "/admin",
                        "outputPath": "dist/apps/admin",
                        "index": "apps/admin/src/index.html",
                        "main": "apps/admin/src/main.ts",
                        "tsConfig": "apps/admin/tsconfig.app.json",
                        "assets": [
                            "apps/admin/src/favicon.ico",
                            "apps/admin/src/assets"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/admin/src/environments/environment.ts",
                                    "with": "apps/admin/src/environments/environment.prod.ts"
                                }
                            ],
                            "mode": "production",
                            "outputHashing": "all",
                            "extractCss": true
                        }
                    }
                },
                "serve": {
                    "builder": "@nx-plus/vue-plugin:dev-server",
                    "options": {
                        "buildTarget": "admin:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "admin:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "config": "apps/admin/.eslintrc",
                        "tsConfig": [
                            "apps/admin/tsconfig.app.json",
                            "apps/admin/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/admin/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/admin/jest.config.js",
                        "tsConfig": "apps/admin/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "admin-e2e": {
            "root": "apps/admin-e2e",
            "sourceRoot": "apps/admin-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/admin-e2e/cypress.json",
                        "tsConfig": "apps/admin-e2e/tsconfig.e2e.json",
                        "devServerTarget": "admin:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "admin:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": ["apps/admin-e2e/tsconfig.e2e.json"],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/admin-e2e/**/*"
                        ]
                    }
                }
            }
        },
        "public": {
            "root": "apps/public",
            "sourceRoot": "apps/public/src",
            "projectType": "application",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "options": {
                        "baseHref": "/public/",
                        "outputPath": "dist/apps/public",
                        "index": "apps/public/src/index.html",
                        "main": "apps/public/src/main.ts",
                        "polyfills": "apps/public/src/polyfills.ts",
                        "tsConfig": "apps/public/tsconfig.app.json",
                        "assets": [
                            "apps/public/src/favicon.ico",
                            "apps/public/src/assets"
                        ],
                        "styles": ["apps/public/src/styles.css"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/public/src/environments/environment.ts",
                                    "with": "apps/public/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "public:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "public:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": [
                            "apps/public/tsconfig.app.json",
                            "apps/public/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/public/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/public/jest.config.js",
                        "tsConfig": "apps/public/tsconfig.spec.json",
                        "passWithNoTests": true,
                        "setupFile": "apps/public/src/test-setup.ts"
                    }
                }
            }
        },
        "public-e2e": {
            "root": "apps/public-e2e",
            "sourceRoot": "apps/public-e2e/src",
            "projectType": "application",
            "architect": {
                "nightly": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/public-e2e/cypress.json",
                        "tsConfig": "apps/public-e2e/tsconfig.e2e.json",
                        "baseUrl": "https://myin142.github.io/"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "tsConfig": ["apps/public-e2e/tsconfig.e2e.json"],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/public-e2e/**/*"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "style": "css",
                "linter": "eslint",
                "babel": true
            },
            "library": {
                "style": "css",
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            },
            "component": {
                "style": "css"
            }
        },
        "@nrwl/next": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/web": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/node": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/nx-plugin": {
            "plugin": {
                "linter": "eslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "admin"
}
